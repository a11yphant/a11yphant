{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:weekly",
    "group:allNonMajor",
    ":semanticCommits"
  ],
  "npmrc": "@a11yphant:registry=https://npm.pkg.github.com/\n//npm.pkg.github.com/:_authToken=${NPM_TOKEN}",
  "encrypted": {
    "npmToken": ""
  },
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchPackageNames": ["node-fetch"],
      "allowedVersions": "!/^2.0.0$/"
    }
  ],
  "prConcurrentLimit": 5,
  "labels": [
    "dependencies"
  ],
  "timezone": "Europe/Vienna"
}
