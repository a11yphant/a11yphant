  "scripts": {
    "prebuild": "npm run schema:generate && npm run gql:generate",
    "vercel-build": "npm run prisma:generate && npm run prisma:migrate:prod && npm run build",
    "build": "next build",
    "predev": "npm run prisma:generate && npm run prisma:migrate",
    "dev": "npm run dotenv -- npm run dev:internal",
    "dev:internal": "concurrently \"next dev -p ${SITE_PORT-3001}\" \"npm run gql:generate:watch\"",
    "dotenv": "dotenv",
    "schema:generate": "ts-node src/api/generate-schema.ts",
    "gql:generate": "graphql-codegen",
    "gql:generate:watch": "graphql-codegen --watch",
    "prelint": "npm run gql:generate",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:eslint": "eslint \"./{src,tests}/**/*.{js,ts,jsx,tsx,json}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "start": "next start -p ${SITE_PORT-3001}",
    "pretest": "npm run schema:generate && npm run gql:generate",
    "test": "jest",
    "pretest:coverage": "npm run schema:generate && npm run gql:generate",
    "test:coverage": "jest --coverage --reporter=default",
    "pretest:debug": "npm run gql:generate",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:watch": "npm run gql:generate",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy"
  },
