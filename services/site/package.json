  "scripts": {
    "dotenv": "dotenv",
    "dev": "npm run dotenv -- npm run dev:internal",
    "dev:internal": "concurrently \"next dev -p ${SITE_PORT-3001}\" \"npm run gql:generate:watch\"",
    "prebuild": "npm run gql:generate",
    "build": "next build",
    "start": "next start -p ${SITE_PORT-3001}",
    "gql:generate": "graphql-codegen",
    "gql:generate:watch": "graphql-codegen --watch",
    "prelint": "npm run gql:generate",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:eslint": "eslint \"./src/**/*.{js,ts,jsx,tsx,json}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "pretest": "npm run gql:generate",
    "test": "jest",
    "pretest:watch": "npm run gql:generate",
    "test:watch": "jest --watch",
    "pretest:coverage": "npm run gql:generate",
    "test:coverage": "jest --coverage --reporter=default",
    "pretest:debug": "npm run gql:generate",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
