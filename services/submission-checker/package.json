{
  "name": "@a11y-challenges/submission-checker",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@axe-core/webdriverjs": "4.1.1",
    "@nestjs/common": "7.6.14",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.14",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "selenium-webdriver": "^4.0.0-alpha.8"
  },
  "devDependencies": {
    "@a11y-challenges/eslint-config": "file:../../packages/eslint-config",
    "@golevelup/nestjs-testing": "0.1.2",
    "@nestjs/cli": "7.5.6",
    "@nestjs/schematics": "7.3.0",
    "@nestjs/testing": "7.6.14",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.35",
    "@types/selenium-webdriver": "4.0.11",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "1.1.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.3",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "cobertura"
    ],
    "coverageDirectory": "<rootDir>"
  }
}
