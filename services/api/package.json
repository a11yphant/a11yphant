{
  "name": "@a11yphant/api",
  "version": "0.0.1",
  "description": "",
  "author": "Michael Hinterhofer | Thomas Dax | Daniela Kubesch | Luca Pircher",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:eslint": "eslint \"**/*.{ts,json}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:unit:watch": "jest tests/unit --watch",
    "test:service": "jest tests/service",
    "test:service:watch": "jest tests/service --watch",
    "test:coverage": "jest --coverage --reporter=default",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "generate-schema": "ts-node src/generate-schema.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "console": "ts-node src/console.ts",
    "console:prod": "node dist/console.js"
  },
  "dependencies": {
    "@apollo/server": "4.5.0",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/apollo": "11.0.4",
