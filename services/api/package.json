{
  "name": "@a11y-challenges/api",
  "version": "0.0.1",
  "description": "",
  "author": "Michael Brandst√§tter | Thomas Dax | Daniela Kubesch | Luca Pircher",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:eslint": "eslint \"**/*.{ts,json}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --reporter=default",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "generate-schema": "ts-node src/generate-schema.ts"
  },
  "dependencies": {
    "@a11y-challenges/prisma": "file:../../packages/prisma",
    "@nestjs/common": "7.6.15",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.15",
    "@nestjs/graphql": "7.10.3",
    "@nestjs/platform-express": "7.6.15",
    "@vendia/serverless-express": "4.3.4",
    "apollo-server-express": "2.22.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@a11y-challenges/eslint-config": "file:../../packages/eslint-config",
    "@golevelup/ts-jest": "0.3.1",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "dotenv": "8.2.0",
    "aws-lambda": "1.0.6",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "1.1.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "cobertura"
    ],
    "globalSetup": "<rootDir>/tests/setup.ts",
    "coverageDirectory": "<rootDir>"
  }
}
