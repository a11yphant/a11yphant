    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "commander": "10.0.0",
    "cookie-parser": "1.4.6",
    "graphql": "16.6.0",
    "handlebars": "4.7.7",
