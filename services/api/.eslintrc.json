{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": ["@a11y-challenges/eslint-config/typescript"],
  "root": true,
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": "off"
  },
  "ignorePatterns": ["**/*.generated.ts", "**/node_modules", "dist/"]
}
