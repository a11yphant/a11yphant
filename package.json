  },
  "keywords": [],
  "_moduleAliases": {
    "@": "src/api/"
  },
  "scripts": {
    "schema:generate": "ts-node --compilerOptions '{\"jsx\": \"react-jsx\"}' src/api/generate-schema.ts",
    "gql:generate": "graphql-codegen",
    "gql:generate:watch": "graphql-codegen --watch",
    "generate": "npm run schema:generate && npm run gql:generate",
    "prelint": "npm run gql:generate",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:content",
    "lint:eslint": "eslint \"./{src,tests}/**/*.{js,ts,jsx,tsx,json}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:content": "yamllint challenges/*.yml",
    "predev": "npm run db:generate && npm run db:migrate",
    "dev": "concurrently \"next dev -p ${SITE_PORT-3001}\" \"npm run gql:generate:watch\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "prebuild": "npm run generate",
    "build": "next build",
    "vercel-build": "npm run build",
    "pretest": "npm run generate",
    "test": "jest",
    "pretest:coverage": "npm run generate",
    "test:coverage": "jest --coverage --reporter=default",
    "pretest:debug": "npm run gql:generate",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:watch": "npm run gql:generate",
    "test:watch": "jest --watch",
    "test:e2e": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:db:reset": "docker compose exec -T postgres psql -U a11yphant -c \"DROP SCHEMA public CASCADE; CREATE SCHEMA public;\" a11yphant",
    "cypress:db-dump:update": "docker compose exec -T postgres pg_dump -U a11yphant a11yphant > db-dump.sql",
    "cypress:db-dump:import": "docker compose exec -T postgres psql -U a11yphant a11yphant < db-dump.sql",
    "start": "next start -p ${SITE_PORT-3001}",
    "console": "ts-node --compilerOptions '{\"jsx\": \"react-jsx\"}' src/api/console.ts",
    "import-challenges": "npm run console import:challenges",
    "delete:staleusers": "npm run console delete:staleusers",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "email": "email dev"
  },
