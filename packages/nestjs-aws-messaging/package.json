{
  "name": "@a11yphant/nestjs-aws-messaging",
  "version": "0.2.2",
  "description": "",
  "author": "Michael Hinterhofer | Thomas Dax | Daniela Kubesch | Luca Pircher",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --reporter=default",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/microservices": "^8.0.6",
    "aws-sdk": "^2.968.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@a11yphant/eslint-config": "0.2.0",
    "@golevelup/ts-jest": "0.3.2",
    "@nestjs/common": "8.2.3",
    "@nestjs/core": "8.2.3",
    "@nestjs/microservices": "8.2.3",
    "@nestjs/testing": "8.2.3",
    "@types/aws-lambda": "8.10.86",
    "@types/jest": "27.0.3",
    "@types/node": "14.18.0",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "aws-lambda": "1.0.7",
    "aws-sdk": "2.1043.0",
    "aws-sdk-mock": "5.5.0",
    "dotenv": "10.0.0",
    "dotenv-cli": "4.1.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "27.4.3",
    "jest-junit": "13.0.0",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "ts-jest": "27.1.0",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testEnvironment": "node",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "cobertura"
    ],
    "coverageDirectory": "<rootDir>/coverage"
  }
}
