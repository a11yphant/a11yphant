{
  "name": "@a11y-challenges/prisma",
  "version": "0.0.1",
  "description": "",
  "author": "Michael Brandst√§tter | Thomas Dax | Daniela Kubesch | Luca Pircher",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "rm -f ./client/query-engine-*",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:tsc": "tsc --project ./tsconfig.lint.json",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "dotenv -e ../../.env prisma migrate dev -- --preview-feature",
    "postinstall": "node scripts/download-engine.js"
  },
  "dependencies": {
    "@prisma/client": "2.18.0",
    "@prisma/engines-version": "2.18.0-6.43e73b6d61a0d707a4b605daa6169624acc33b74",
    "@prisma/fetch-engine": "2.18.0",
    "@prisma/migrate": "2.18.0",
    "@prisma/sdk": "2.18.0"
  },
  "peerDependencies": {
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.6"
  },
  "devDependencies": {
    "@a11y-challenges/eslint-config": "file:../eslint-config",
    "@golevelup/ts-jest": "0.3.1",
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "@nestjs/testing": "7.6.13",
    "@types/jest": "26.0.20",
    "@types/mock-fs": "4.13.0",
    "@types/node": "14.14.25",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "dotenv": "8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "1.1.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma": "2.18.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.6",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/test/setup.ts"
  }
}
